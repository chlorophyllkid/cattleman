  "repository": {
    "type": "git",
    "url": "git+https://github.com/chlorophyllkid/cattleman.git"
  },
  "bugs": {
    "url": "https://github.com/chlorophyllkid/cattleman/issues"
  },
  "keywords": [
    "webpack",
    "modular",
    "entries",
    "bundling",
    "helper"
  ],
  "dependencies": {},
  "devDependencies": {
    "coveralls": "2.13.1",
    "istanbul": "0.4.5",
    "tap-spec": "4.1.1",
    "tape": "4.7.0"
  }
}
