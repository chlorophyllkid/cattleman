  "description": "A Webpack helper library that gathers entry chunks and templates.",
  "license": "MIT",
  "author": "Christopher Voigt <chlorophyllkid@gmail.com>",
  "main": "lib/cattleman.js",
  "scripts": {
    "test": "./node_modules/.bin/tape tests/**/*.js | tap-spec",
